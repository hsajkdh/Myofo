<!--pages/billing/index.wxml-->
<view class='continer'>
   <view class='number'>
     <text>当前单车编号：{{number}}</text>
   </view>
   <view class='time'>
      <view class='time-title'>
          {{title}}：
      </view>
      <view class='time-content'>
          {{hours}}: {{minutes}}: {{seconds}}
      </view>
   </view>
   <view class='endride'>
       <button type='warn' bindtap='end' disabled='{{overclick}}'>结束骑行</button>
       <button type='primary' bindtap='moveToMap'>返回地图</button>
   </view>
</view>
